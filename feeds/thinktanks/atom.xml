---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>{{ site.title }} News</title>
  <link rel="self" href="{{ site.url }}/atom.xml"/>
  <author><name>Kin Lane</name></author>
  <id>{{ site.url }}/atom.xml</id>
	{% for post in site.data.thinktanks limit:25 %}<entry>
    <title>{{ post.Name }}</title>
    <link href="{{ post.URL }}"/>
    <updated>{{ post.item_date | date: "%Y-%m-%dT%H:%M:%SZ" }}</updated>
    <content></content>
    <id>{{ site.url }}{{ post.url }}</id>
  </entry>{% endif %}{% endfor %}
</feed>
